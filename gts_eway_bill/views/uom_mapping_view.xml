<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <record id="tree_uom_mapping_code" model="ir.ui.view">
        <field name="name">uom.mapping.tree</field>
        <field name="model">uom.mapping</field>
        <field name="arch" type="xml">
            <tree string="UOM Mapping" editable="bottom">
                <field name="name"/>
                <field name="code"/>
            </tree>
        </field>
    </record>

    <record model="ir.actions.act_window" id="action_uom_mapping">
        <field name="name">Uom Mapping</field>
        <field name="res_model">uom.mapping</field>
        <field name="type">ir.actions.act_window</field>
        <field name="view_mode">tree</field>
    </record>

<!--    <menuitem id="menu_action_uom_mapping" parent="gst_ewaybill" action="action_uom_mapping"/>-->
</odoo>
